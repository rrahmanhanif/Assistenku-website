{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.css",  "use": "@vercel/static" },
    { "src": "*.js",   "use": "@vercel/static" },
    { "src": "*.{png,jpg,jpeg,svg,webp}", "use": "@vercel/static" }
  ]
}
